{% include 'object-filters-demo/styles' %}
<section class="object-filters">
  <input class="collapsible__checkbox" type="checkbox" id="collapsible-checkbox" />
  <label for="collapsible-checkbox" class="collapsible__toggle"></label>
  <div class="collapsible__panel">
    <div class="object-filters__list">
      {% for i in (1..30) %}
        <div class="object-filters__item">
          <label for="object-type-{{i}}">Object Filter {{i}}:</label>
          <select name="Type" id="object-type-{{i}}">
            <option value="">All</option>
            <option value="something">Something</option>
          </select>
        </div>
      {% endfor %}
      <div class="object-filters__item">
        <label for="object-period">Ut placerat orci nulla pellentesque dignissim enim, sit amet venenatis urna cursus eget nunc</label>
        <select name="Time Period" id="object-period">
          <option value="">All</option>
          <option value="something">Sed id semper risus in hendrerit gravida rutrum quisque</option>
        </select>
      </div>
    </div>
    <button class="object-filters__button">Reset Filters</button>
  </div>
</section>
<script type="text/javascript">
const classes = [
  'collapsible',
  // 'sticky'
];
</script>
{% include 'object-filters-demo/script' %}
